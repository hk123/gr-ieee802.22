<?xml version="1.0"?>

<block>
	<name>OFDM Parse MAC</name>
	<key>ieee802_22_ofdm_parse_mac</key>
	<category>IEEE802.22</category>
	<import>import ieee802_22</import>
	<make>ieee802_22.ofdm_parse_mac($log, $debug)</make>

	<param>
		<name>Log</name>
		<key>log</key>
		<value>False</value>
		<type>bool</type>

		<option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
	</param>

	<param>
		<name>Debug</name>
		<key>debug</key>
		<value>False</value>
		<type>bool</type>

		<option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
	</param>

	<sink>
		<name>in</name>
		<type>message</type>
	</sink>

	<source>
		<name>fer</name>
		<type>message</type>
		<optional>1</optional>
	</source>

</block>

